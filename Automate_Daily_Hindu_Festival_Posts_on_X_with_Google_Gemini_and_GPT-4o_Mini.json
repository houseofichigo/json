      ],
      "parameters": {
        "jsonSchemaExample": "[\n  {\n    \"text\": \"...\",\n    \"hashtags\": \"#.. #.. #..\",\n    \"call_to_action\": \"...\"\n  },\n  {\n    \"text\": \"...\",\n    \"hashtags\": \"#.. #.. #..\",\n    \"call_to_action\": \"...\"\n  }\n]"
      },
